{
   "name": "nodejs-server-employees",
   "version": "0.1.08",
   "description": "Server side application for employees app",
   "engines": {
      "node": "^18.0.0",
      "npm": "^8.0.0"
   },
   "main": "./src/server.ts",
   "scripts": {
      "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
      "client-install": "npm install --prefix client",
      "start": "cp ./env/env-dev ./.env && ts-node ./src/server.ts",
      "dockerize": "cp ./env/env-docker ./.env && ts-node ./src/server.ts ",
      "server": "nodemon ./src/server.ts --ignore client ",
      "server2": "cp ./env/env-dev ./.env && nodemon ./src/server.ts --ignore client ",
      "format": "prettier --write **/*.ts",
      "format-check": "prettier --check **/*.ts",
      "test": "jest --coverage",
      "test2": "jest --detectOpenHandles",
      "pre-checkin": "npm run format && npm run build && npm test",
      "lint": "eslint src/**/*.ts"
   },
   "adminStart": true,
   "keywords": [
      "nodejs",
      "jwt",
      "passport",
      "express"
   ],
   "author": "mdnelles",
   "license": "ISC",
   "dependencies": {
      "@actions/github": "^6.0.0",
      "@babel/preset-env": "^7.23.3",
      "@types/express": "^4.17.21",
      "@types/mongodb": "^4.0.7",
      "bcrypt": "^5.1.1",
      "body-parser": "^1.20.2",
      "cors": "*",
      "D": "^1.0.0",
      "dotenv": "^16.3.1",
      "errorhandler": "^1.5.1",
      "express": "*",
      "express-session": "^1.17.3",
      "flatted": "^3.2.9",
      "formidable": "^3.5.1",
      "fs-extra": "^11.1.1",
      "helmet": "^7.1.0",
      "jsonwebtoken": "^9.0.2",
      "nodemailer": "^6.9.7",
      "serve": "^14.2.1",
      "shelljs": "^0.8.5",
      "supertest": "^6.3.3",
      "ts-node": "^10.9.1",
      "uuid": "^9.0.1"
   },
   "devDependencies": {
      "@babel/preset-typescript": "^7.23.3",
      "@jest/globals": "^29.7.0",
      "@shelf/jest-mongodb": "^4.1.7",
      "@types/bcrypt": "^5.0.2",
      "@types/body-parser": "^1.19.5",
      "@types/compression": "^1.7.5",
      "@types/cors": "^2.8.16",
      "@types/express": "^4.17.21",
      "@types/express-session": "^1.17.10",
      "@types/jest": "^29.5.8",
      "@types/jsonwebtoken": "^9.0.5",
      "@types/node": "^20.9.1",
      "@types/nodemailer": "^6.4.14",
      "@types/supertest": "^2.0.16",
      "@typescript-eslint/eslint-plugin": "^6.11.0",
      "@typescript-eslint/parser": "^6.11.0",
      "eslint": "^8.53.0",
      "eslint-plugin-react": "^7.33.2",
      "jest": "^29.7.0",
      "jest-circus": "^29.7.0",
      "nodemon": "^3.0.1",
      "prettier": "^3.1.0",
      "ts-jest": "^29.1.1",
      "tslib": "^2.6.2",
      "typescript": "^5.2.2"
   }
}
