{
   "name": "nodejs-server-employees",
   "version": "0.1.08",
   "description": "Server side application for employees app",
   "engines": {
      "node": "^18.0.0",
      "npm": "^8.0.0"
   },
   "main": "./src/server.ts",
   "scripts": {
      "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
      "client-install": "npm install --prefix client",
      "start": "cp ./env/env-dev ./.env && ts-node ./src/server.ts",
      "dockerize": "cp ./env/env-docker ./.env && ts-node ./src/server.ts ",
      "server": "nodemon ./src/server.ts --ignore client ",
      "server2": "cp ./env/env-dev ./.env && nodemon ./src/server.ts --ignore client ",
      "format": "prettier --write **/*.ts",
      "format-check": "prettier --check **/*.ts",
      "test": "jest --coverage",
      "pre-checkin": "npm run format && npm run build && npm test",
      "lint": "eslint src/**/*.ts"
   },
   "adminStart": true,
   "keywords": [
      "nodejs",
      "jwt",
      "passport",
      "express"
   ],
   "author": "mdnelles",
   "license": "ISC",
   "dependencies": {
      "@actions/github": "^5.1.1",
      "@babel/preset-env": "^7.21.5",
      "@types/express": "^4.17.17",
      "@types/mongodb": "^4.0.7",
      "bcrypt": "^5.1.0",
      "body-parser": "^1.20.2",
      "cors": "*",
      "D": "^1.0.0",
      "dotenv": "^16.0.3",
      "errorhandler": "^1.5.1",
      "express": "*",
      "express-session": "^1.17.3",
      "flatted": "^3.2.7",
      "formidable": "^2.1.1",
      "fs-extra": "^11.1.1",
      "helmet": "^6.1.5",
      "jsonwebtoken": "^9.0.0",
      "nodemailer": "^6.9.1",
      "serve": "^14.2.0",
      "shelljs": "^0.8.5",
      "supertest": "^6.3.3",
      "ts-node": "^10.9.1",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "@babel/preset-typescript": "^7.21.5",
      "@jest/globals": "^29.5.0",
      "@shelf/jest-mongodb": "^4.1.7",
      "@types/bcrypt": "^5.0.0",
      "@types/body-parser": "^1.19.2",
      "@types/compression": "^1.7.2",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/express-session": "^1.17.7",
      "@types/jest": "^29.5.1",
      "@types/jsonwebtoken": "^9.0.1",
      "@types/node": "^18.16.0",
      "@types/nodemailer": "^6.4.7",
      "@types/supertest": "^2.0.12",
      "@typescript-eslint/eslint-plugin": "^5.59.1",
      "@typescript-eslint/parser": "^5.59.1",
      "eslint": "^8.39.0",
      "eslint-plugin-react": "^7.32.2",
      "jest": "^29.5.0",
      "jest-circus": "^29.5.0",
      "nodemon": "^2.0.22",
      "prettier": "^2.8.8",
      "ts-jest": "^29.1.0",
      "tslib": "^2.5.0",
      "typescript": "^5.0.4"
   }
}
